<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:horizontalGap="0px"
    android:keyHeight="@dimen/key_symbol_height"
    android:keyWidth="25%p"
    android:verticalGap="0px">

    <Row>
        <Key android:codes="40" android:keyLabel="(" android:keyEdgeFlags="left" />
        <Key android:codes="47" android:keyLabel="/" />
        <Key android:codes="41" android:keyLabel=")"/>
        <Key android:codes="48" android:keyLabel="-" android:keyEdgeFlags="right" />
    </Row>

    <Row>
        <Key android:codes="78" android:keyLabel="N" android:keyEdgeFlags="left" />
        <Key android:codes="44" android:keyLabel="Pause" />
        <Key android:codes="44" android:keyLabel="," />
        <Key android:codes="46" android:keyLabel="." android:keyEdgeFlags="right" />
    </Row>
    
    <Row>
        <Key android:codes="42" android:keyLabel="*" android:keyEdgeFlags="left" />
        <Key android:codes="59" android:keyLabel="Wait" />
        <Key android:codes="35" android:keyLabel="\#" />
        <Key android:codes="-5" android:keyIcon="@drawable/ic_backspace_24dp"
            android:keyEdgeFlags="right" />
    </Row>
    
    <Row android:rowEdgeFlags="bottom">
        <Key android:codes="-2" android:keyLabel="123" android:keyEdgeFlags="left" />
        <Key android:codes="43" android:keyLabel="+"/>
        <Key android:codes="32" android:keyIcon="@drawable/ic_space_bar_24dp" />
        <Key android:codes="10" android:keyIcon="@drawable/ic_keyboard_return_24dp"
            android:isRepeatable="true" android:keyEdgeFlags="right" />
    </Row>
</Keyboard>